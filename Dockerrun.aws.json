{
    "AWSEBDockerrunVersion" : 2,
    "containerDefinitions": [
        {
            "name": "client", 
            "image": "23david73/multi-client",
            "hostname": "client", 
            "essential": false
        },
        {
            "name": "server", 
            "image": "23david73/multi-server",
            "hostname": "api", 
            "essential": false
        }, 
        {
            "name": "worker", 
            "image": "23david73/multi-worker",
            "hostname": "worker", 
            "essential": false
        },
        {
            "name": "nginx", 
            "image": "23david73/multi-nginx", 
            "essential": true, 
            "portMappings": [
                {
                    "hostPort": 80, 
                    "containerPort": 80
                }
            ], 
            "links": [
                "client", 
                "server"
            ]
        }
    ]
}